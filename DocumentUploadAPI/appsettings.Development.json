{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "ApplicationInsights": {
    "ConnectionString": ""
  },
  "OpenTelemetry": {
    "EnableConsoleExporter": true,
    "EnableFileExporter": false,
    "LogDirectory": "./logs"
  },
  "FileUpload": {
    "StoragePath": "./uploads",
    "TempPath": "./temp"
  }
}
